{
    "version": 2,
    "builds": [
      {
        
        "src": "server.js",
        
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          
            "src": "/",
            
            "dest": "server.js"
          },
        {
          
            "src": "/deleteNote/:email/:title",
            
            "dest": "server.js"
          },
          {
            "src": "/note/:email",
            
            "dest": "server.js"
          },
          {
            "src": "/saveNote/:email/:title/:description",
            
            "dest": "server.js"
          },
          {
            "src": "/allNotes/:email",
            
            "dest": "server.js"
          }
      ]
  }